{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "MySequelConnection": "${MY_SEQUEL_CONNECTION}"
  },
  "Configuration": {
    "DbVersion": "8.0.29",
    "SmtpSender": "info@quizazu.com",
    "SmtpLoopback": "info@quizazu.com",
    "SmtpName": "Quizazu",
    "SmtpHost": "${SMTP_HOST}",
    "SmtpPort": 25,
    "SmtpPassword": "${SMTP_PASSWORD}",
    "JwtExpiredDays": 30
  }
}
